{
  "db_name": "PostgreSQL",
  "query": "SELECT\n            work_id,\n            work_name,\n            work_version,\n            work_platform,\n            replay_id,\n            replay_name,\n            replay_description,\n            file_id,\n            instance_id\n            FROM work JOIN instance USING (work_id)\n            JOIN replay USING (instance_id)\n            WHERE replay.creator_id = $1 AND f_unaccent(work_name || replay_name || replay_description) ILIKE ('%' || f_unaccent($2) || '%')\n            OFFSET $3\n            LIMIT $4",
