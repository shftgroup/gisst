{
  "db_name": "PostgreSQL",
  "query": "SELECT\n            work_id,\n            work_name,\n            work_version,\n            work_platform,\n            state_id,\n            state_name,\n            state_description,\n            screenshot_id,\n            file_id,\n            instance_id\n            FROM work JOIN instance USING (work_id)\n            JOIN state USING (instance_id)\n            WHERE state.creator_id = $1 AND f_unaccent(work_name || state_name || state_description) ILIKE ('%' || f_unaccent($2) || '%')\n            ORDER BY state.created_on DESC\n            OFFSET $3\n            LIMIT $4",
