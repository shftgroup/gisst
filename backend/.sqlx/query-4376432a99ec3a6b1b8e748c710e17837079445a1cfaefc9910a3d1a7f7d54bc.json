  "db_name": "PostgreSQL",
  "query": "SELECT state_id,\n            instance_id,\n            is_checkpoint,\n            file_id,\n            state_name,\n            state_description,\n            screenshot_id,\n            replay_id,\n            creator_id,\n            state_replay_index,\n            state_derived_from,\n            created_on\n            FROM state\n            WHERE instance_id = $1 AND f_unaccent(state_name || state_description) ILIKE ('%' || f_unaccent($2) || '%')\n            ORDER BY state.created_on DESC\n            OFFSET $3\n            LIMIT $4",
