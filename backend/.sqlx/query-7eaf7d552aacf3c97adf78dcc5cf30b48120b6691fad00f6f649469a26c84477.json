  "query": "SELECT replay_id,\n            replay_name,\n            replay_description,\n            instance_id,\n            creator_id,\n            file_id,\n            replay_forked_from,\n            created_on\n            FROM replay\n            WHERE POSITION($1 in replay_name) > 0 OR POSITION($1 in replay_description) > 0\n            ORDER BY $2 ASC\n            LIMIT $3\n            OFFSET $4\n            ",
